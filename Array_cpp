//Array CPP 
#include <stdio.h>
template<class T>
class Array
{
 private:
  T *A;
  int size;
  int length;
public:
 Array()
  {
   size=10;
   A=new T[10];
   length=0;
  }
 Array(sz);
 {
  size=sz;
  A=new T[size];
  length=0;
}
~Array()
 {
  delete T A;
 }
 void Display();
 void Insert(int index,T x);
 T Delete(int index);

}

template<class T>
void Array<T>::Display()
{
 for(int i=0;i<length;i++)
  cout<<A[i]<<" ";
cout<<end;
}

template<class T>
void Array<T>::Insert(int index,int x)
{
 for(i=index+1;i<length+1;i++)
  A[i+1]=A[i];
A[i]=x;
length++
}

template<class T>
int Delete<T>::Delete(int index);
{
 int x=A[index]
 for(i=index+1;i<length-1;i++)
   A[i]=A[i-1];
 return x;
}

int main()
{
  Array<char> arr(10);

  arr.Insert(0,5);
  arr.Delete(1,6);
  arr.Insert(1,6);
  arr.Display();
  return 0;
}
